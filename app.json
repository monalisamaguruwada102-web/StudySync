{
  "expo": {
    "name": "OFF REZ CONNECT",
    "slug": "off-rez-connect",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon_fixed.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash_fixed.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.offrez.connect"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_icon_fixed.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.offrez.connect"
    },
    "web": {
      "favicon": "./assets/icon_fixed.png"
    },
    "plugins": [
      "@react-native-community/datetimepicker",
      [
        "expo-build-properties",
        {
          "android": {
            "kotlinVersion": "2.1.0",
            "compileSdkVersion": 35,
            "targetSdkVersion": 35,
            "buildToolsVersion": "35.0.0"
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "dbf18997-d771-416f-b7a5-95cde9eb9468"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/dbf18997-d771-416f-b7a5-95cde9eb9468"
    }
  }
}